{
  "name": "vue-chart-3",
  "version": "0.5.11",
  "description": "ðŸ“Š A simple wrapper around Chart.js 3 for Vue 2 & 3",
  "keywords": [
    "vue chartjs 3",
    "vue chart.js 3",
    "chartjs 3",
    "vue chartjs",
    "chartjs vue",
    "chartjs 3 vue",
    "chartjs vue3",
    "chartjs vue 3",
    "vue chart.js"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf types",
    "compile": "tsc --pretty -p tsconfig.build.json && cp -r ./dist ./debug/vue2/src && cp -r ./dist ./debug/vue3/src",
    "dev": "tsc --pretty --watch",
    "build": "yarn run clean && yarn run compile",
    "debug:vue2": "cd debug/vue2 && yarn serve",
    "debug:vue3": "cd debug/vue3 && yarn serve",
    "test": "vue-demi-switch 2 && karma start",
    "docs:dev": "vuepress dev docs",
    "docs:build": "yarn run post:docs && vuepress build docs",
    "post:docs": "echo \\$contentClass = \\'.content__default\\' > ./node_modules/@vuepress/theme-default/styles/config.styl",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "peerDependencies": {
    "@vue/composition-api": "^1.0.4",
    "chart.js": "^3.1.0",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@vue/runtime-core": "3.2.20",
    "@vue/runtime-dom": "3.2.20",
    "csstype": "3.0.9",
    "lodash": "latest",
    "nanoid": "3.1.30",
    "vue-demi": "^0.10.1"
  },
  "devDependencies": {
    "@testing-library/vue": "^5.8.2",
    "@types/chart.js": "^2.9.34",
    "@types/jasmine": "^3.10.0",
    "@types/karma": "^6.3.1",
    "@types/lodash": "^4.14.175",
    "@types/node": "^14.0.27",
    "@vue/cli": "^4.5.14",
    "@vue/composition-api": "^1.0.4",
    "@vue/test-utils": "^1.2.2",
    "canvas": "^2.8.0",
    "chart.js": "^3.5.1",
    "chartjs-test-utils": "^0.3.1",
    "copyfiles": "^2.4.1",
