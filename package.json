    "karma-typescript-es6-transform": "^5.5.2",
    "lerna": "^4.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4",
    "vue": "^2.6.14",
